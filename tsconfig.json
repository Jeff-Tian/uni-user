{
  "compilerOptions": {
    "noImplicitAny": false,
    "removeComments": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "esModuleInterop": true,
    "outDir": "lib",
    "sourceMap": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "files": [
    "src/api/api.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "awesomeTypescriptLoaderOptions": {}
}