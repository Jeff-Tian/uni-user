  "scripts": {
    "local": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "dev": "cross-env webpack-dev-server ---config webpack.dev.config.js",
    "build": "cross-env CSS_LOADER_EXCLUDE_NODE_MODULES=false webpack",
    "test": "mocha -r ./babel-register.js test/feature.test.ts --timeout=10000",
    "babel": "babel --presets @babel/preset-typescript test/feature.test.ts",
    "rollup": "rollup src/index.js --file lib/bundle.js --format umd --name uni-user",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "storybook": "cross-env CSS_LOADER_EXCLUDE_NODE_MODULES=true start-storybook -p 6006",
    "build-storybook": "cross-env CSS_LOADER_EXCLUDE_NODE_MODULES=true build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "semantic-release": "semantic-release",
    "profile": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
